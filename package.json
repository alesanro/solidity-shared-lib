{
  "name": "solidity-shared-lib",
  "version": "0.2.1",
  "description": "Specifies contracts shared between libraries",
  "main": "truffle.js",
  "scripts": {
    "test:all": "npx truffle test --network test",
    "run:rpc": "npx ganache-cli -g 1 --gasLimit 4700000 --trace_request true | grep -Ev \"FilterSubprovider|eth_getFilterChanges\"",
    "compile": "npx truffle compile",
    "compile:all": "npx truffle compile --all",
    "migrate": "npx truffle migrate",
    "migrate:reset": "npx truffle migrate --reset",
    "console": "npx truffle console --network test"
  },
  "devDependencies": {
    "ganache-cli": "6.1.0",
    "truffle": "4.1.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChronoBank/solidity-shared-lib.git"
  },
  "author": "LaborX PTY",
  "bugs": {
    "url": "https://github.com/ChronoBank/solidity-shared-lib/issues"
  },
  "homepage": "https://github.com/ChronoBank/solidity-shared-lib#readme",
  "license": "AGPL-3.0"
}
